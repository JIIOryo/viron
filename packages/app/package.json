{
  "name": "@viron/app",
  "version": "2.0.0",
  "scripts": {
    "pre-commit": "lint-staged",
    "pre-push": "npm run test",
    "preflight": "npm install --legacy-peer-deps",
    "develop": "gatsby develop --https -p 8000",
    "build": "gatsby build",
    "serve": "gatsby serve --https -p 8000",
    "clean": "gatsby clean",
    "format": "run-p format:*",
    "format:tjs": "prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "run-p lint:*",
    "lint:tjs": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "test": "run-p typecheck",
    "typecheck": "tsc",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^2.0.0",
    "@reach/router": "^1.3.4",
    "@react-icons/all-files": "^4.1.0",
    "classnames": "^2.2.6",
    "gatsby": "^3.0.3",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-lodash": "^4.0.0",
    "gatsby-plugin-page-creator": "^3.0.0",
    "gatsby-plugin-postcss": "^4.0.0",
    "i18next": "^20.0.0",
    "i18next-browser-languagedetector": "^6.1.0",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.7",
    "postcss-preset-env": "^6.7.0",
    "query-string": "^6.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.0.2",
    "react-i18next": "^11.8.10",
    "recoil": "^0.1.3",
    "semver": "^7.3.4",
    "store2": "^2.12.0",
    "tailwindcss": "^2.0.3",
    "webpack": "^5.24.4",
    "yup": "^0.32.9"
  },
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@hookform/devtools": "^3.0.1",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/classnames": "^2.2.11",
    "@types/react-dom": "^17.0.2",
    "@types/recoil": "^0.0.1",
    "@types/semver": "^7.3.4",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
